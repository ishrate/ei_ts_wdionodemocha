{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["esnext", "dom"],
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "typeRoots": ["./node_modules/@types", "./test/types"],
    "types": [
      "node",
      "mocha",
      "chai",
      //"webdriverio",
      //"@wdio/types"
      
      //"@wdio/sync"
    ]
  },
  "include": [
    "test/**/*",
    "wdio.conf.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
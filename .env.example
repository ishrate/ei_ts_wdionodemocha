# Environment Configuration
NODE_ENV=dev
ENVIRONMENT=dev

# ===========================================
# MULTIPLE AUT (APPLICATION UNDER TEST) CONFIGURATION
# ===========================================

# Default/Primary Application URLs
BASE_URL=https://your-primary-app.com
DEV_URL=https://dev.your-app.com
QA_URL=https://qa.your-app.com
STAGING_URL=https://staging.your-app.com
PROD_URL=https://prod.your-app.com

# ===========================================
# AUT 1: MEDIPASS APPLICATION
# ===========================================
MEDIPASS_BASE_URL=https://stg-connect.medipass.io
MEDIPASS_LOGIN_URL=https://stg-connect.medipass.io/login
MEDIPASS_DEV_URL=https://dev-connect.medipass.io
MEDIPASS_QA_URL=https://qa-connect.medipass.io
MEDIPASS_STAGING_URL=https://stg-connect.medipass.io
MEDIPASS_PROD_URL=https://prod-connect.medipass.io

# ===========================================
# AUT 2: DUCKDUCKGO (SEARCH ENGINE)
# ===========================================
DUCKDUCKGO_BASE_URL=https://duckduckgo.com
DUCKDUCKGO_SEARCH_URL=https://duckduckgo.com

# ===========================================
# AUT 3: AVAILABLE FOR FUTURE USE
# ===========================================
# Add your third application URLs here when needed

# Legacy support (for backward compatibility)
SEARCH_URL=https://duckduckgo.com

# Browser Settings
BROWSER=chrome
MAXIMIZE_BROWSER=true
HEADLESS=false

# Timeouts (in milliseconds)
PAGE_LOAD_TIMEOUT=10000
ELEMENT_WAIT_TIMEOUT=5000
SEARCH_RESULTS_TIMEOUT=15000

# Test Data
DEFAULT_SEARCH_TERM=cheese
XML_DATA_FILE=searchData.xml

# Login Credentials (Replace with actual values)
# Note: Never commit real credentials to version control
TEST_USERNAME=your-test-username
TEST_PASSWORD=your-test-password
ADMIN_USERNAME=your-admin-username
ADMIN_PASSWORD=your-admin-password

# Environment-specific credentials
DEV_USERNAME=your-dev-username
DEV_PASSWORD=your-dev-password
QA_USERNAME=your-qa-username
QA_PASSWORD=your-qa-password
STAGING_USERNAME=your-staging-username
STAGING_PASSWORD=your-staging-password
